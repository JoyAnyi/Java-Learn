class Main {
    public static void main(String[] args) {
      
        String[] names =  {"Joy", "Jane", "Alice", "John Doe", "Johnny", "Matthew", "Solomon"};
        for(String name : names){
            System.out.println(name);
        }
       int countStartsWithJ = 0;
       String longest = names[0];
       
       for(String name : names){
           if(name.startsWith("J")){
               countStartsWithJ++;
           }
           if(name.startsWith("J")){
              System.out.println("Names which start with letter J: " + name);
           }
           if(name.length() > longest.length()){
               longest = name;
           }

       }
        System.out.println("Number of names starting with 'J': " + countStartsWithJ);
        
         System.out.println("Longest name: " + longest);
    }
}
